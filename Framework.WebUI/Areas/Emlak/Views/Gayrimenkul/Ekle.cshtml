    <script type="text/javascript">
        $(document).ready(function () {

            $("#Il_Id").change(function (e) {

                var ilId = $(this).val();

                if (ilId != null) {
                    $.ajax({
                        type: 'Post',
                        url: '/Gayrimenkul/IlceSelectList/ilId=' + ilId,
                        data: { ilId: ilId },
                        success: function (result) {
                            if (result != null && result.success) {
                                $("#Ilce_Id").empty();
                                $("#Ilce_Id").append(
                                    $('<option></option>').val(null).html('Lütfen Seçiniz'));
                                $.each(result.Data, function (i, data) {
                                    $('<option></option>').val(null).html('');
                                    $("#Ilce_Id").append(
                                        $('<option></option>').val(data.Id).html(data.Ad));
                                });
                            }
                            else {
                                swal('Uyarı!', result.Message, 'warning');
