
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Kira/Views/Shared/_LayoutKira.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> SmartAdmin - Responsive admin template..</title>
    <link rel="shortcut icon" href="img/favicon.ico">
    <!--STYLESHEET-->
    <!--=================================================-->
    

    <!--Jasmine Stylesheet [ REQUIRED ]-->
    <link href="https://localhost:44336/Content/css/style.css" rel="stylesheet">
    <!--Switchery [ OPTIONAL ]-->
    <link href="https://localhost:44336/Content/css/switchery.min.css" rel="stylesheet">
    <!--Bootstrap Select [ OPTIONAL ]-->
    <link href="https://localhost:44336/Content/css/bootstrap-select.min.css" rel="stylesheet">
    <!--Demo [ DEMONSTRATION ]-->
    <link href="https://localhost:44336/Content/css/jasmine.css" rel="stylesheet">
    <!--SCRIPT-->
    <!--=================================================-->
    <!--Page Load Progress Bar [ OPTIONAL ]-->
    <link href="https://localhost:44336/Content/css/pace.min.css" rel="stylesheet">
    <script src="https://localhost:44336/Scripts/jsdashboard/pace.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" />
</head>
<body>
    <div id="content">
        <section id="widget-grid" class="sayfaust_bosluk">
            <div class="row">
                <article class="col-sm-12 col-md-12 col-lg-12">
                    <div class="jarviswidget" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-togglebutton="false" data-widget-deletebutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-sortable="false">
                        <header>
                            <span class="widget-icon"> <i class="fa fa-list"></i> </span>
                            <h2> <b>Kira Beyan Verileri</b></h2>

                        </header>
                        <div role="contentinfo">
                            <div id="page-content">
                                <!--Widget-4 -->
                                <div class="row">
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="panel">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-md-9 col-sm-9 col-xs-10">
                                                        <h3 class="mar-no"> <span class="counter">2370</span></h3>
                                                        <p class="mar-ver-5"> Taşınmaz Sayısı </p>
                                                    </div>
                                                    <div class="col-md-3 col-sm-3 col-xs-2"> <i class="fa fa-home fa-3x text-info"></i> </div>
                                                </div>
                                                <div class="progress progress-striped progress-sm">
                                                    <div style="width: 60%;" aria-valuemax="100" aria-valuemin="0" aria-valuenow="60" role="progressbar" class="progress-bar"> <span class="sr-only">60% Complete</span> </div>
                                                </div>
                                                <p> %70 Aktif Kiralık </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="panel">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-md-9 col-sm-9 col-xs-10">
                                                        <h3 class="mar-no"> <span class="counter">630</span></h3>
                                                        <p class="mar-ver-5"> Taşınmaz Sayısı </p>
                                                    </div>
                                                    <div class="col-md-3 col-sm-3 col-xs-2"> <i class="fa fa-home fa-3x text-info"></i> </div>
                                                </div>
                                                <div class="progress progress-striped progress-sm">
                                                    <div style="width: 60%;" aria-valuemax="100" aria-valuemin="0" aria-valuenow="60" role="progressbar" class="progress-bar"> <span class="sr-only">60% Complete</span> </div>
                                                </div>
                                                <p> %30 Pasif Kiralık </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="panel widget">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-md-9 col-sm-9 col-xs-10">
                                                        <h3 class="mar-no"> <span class="counter">14,500</span></h3>
                                                        <p class="mar-ver-5"> Toplam Beyan Sayısı </p>
                                                    </div>
                                                    <div class="col-md-3 col-sm-3 col-xs-2"> <i class="fa fa-file fa-3x text-success"></i> </div>
                                                </div>
                                                <div class="progress progress-striped progress-sm">
                                                    <div style="width: 60%;" aria-valuemax="100" aria-valuemin="0" aria-valuenow="60" role="progressbar" class="progress-bar progress-bar-success"> <span class="sr-only">60% Complete</span> </div>
                                                </div>
                                                <p> %100 Beyan </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="panel widget">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-md-9 col-sm-9 col-xs-10">
                                                        <h3 class="mar-no"> <span class="counter">600</span></h3>
                                                        <p class="mar-ver-5"> Ödemesi Gelen Beyan Sayısı</p>
                                                    </div>
                                                    <div class="col-md-3 col-sm-3 col-xs-2"> <i class="fa fa-credit-card fa-3x text-info"></i> </div>
                                                </div>
                                                <div class="progress progress-striped progress-sm">
                                                    <div style="width: 60%;" aria-valuemax="100" aria-valuemin="0" aria-valuenow="60" role="progressbar" class="progress-bar progress-bar-warning"> <span class="sr-only">60% Complete</span> </div>
                                                </div>
                                                <p> 20% Ödemesi Gelen Beyan  </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </article>
            </div>
        </section>

    </div>

    <!--===================================================-->
    <!-- END OF CONTAINER -->
    <!--JAVASCRIPT-->
    <!--=================================================-->
    <!--BootstrapJS [ RECOMMENDED ]-->
  
</body>
</html>

@section pagespecific {
    @Scripts.Render("~/bundles/jqueryval")
}
