                                                                                    <tr role="row" class="odd">
                                                                                        <td>@Html.DisplayFor(model => item.Kira_Beyani.Beyanlar.BeyanYil)</td>
                                                                                        <td>@Html.DisplayFor(model => item.TaksitSayisi)</td>
                                                                                        <td>@Html.DisplayFor(model => item.TahakkukTarihi)</td>
                                                                                        <td>@Html.DisplayFor(model => item.VadeTarihi)</td>
                                                                                        <td>@Html.DisplayFor(model => item.Tutar)</td>
                                                                                        <td>@Html.DisplayFor(model => item.Aciklama)</td>
                                                                                        <td>
                                                                                            @if (item.OdemeDurumu.HasValue && item.OdemeDurumu.Value)
                                                                                            {
                                                                                                <b class="label aktif-buton-beyandetay">Tahsil </b>
                                                                                            }
                                                                                            else
                                                                                            {
                                                                                                <b class="label pasif-buton-beyandetay">Vade</b>
                                                                                            }
                                                                                        </td>
                                                                                        <td>
                                                                                            @if (item.KiraParametreKodu == 626)
                                                                                            {
                                                                                                var taksit = Model.Tahakkuklar.Where(a => a.TaksitSayisi == item.TaksitSayisi && a.EkTahakkukMu.Value == true).GroupBy(a => a.TaksitSayisi);

                                                                                                if (taksit != null && taksit.Count() > 0)
