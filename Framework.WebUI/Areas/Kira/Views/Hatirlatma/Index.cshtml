@model Framework.WebUI.Models.ViewModels.HatirlatmaListesiVM
@{
    ViewBag.Title = "Hatırlatma Listesi";
    Layout = "~/Areas/Kira/Views/Shared/_LayoutKira.cshtml";
}
@if (!Model.HideContent)
{
    <div id="content">
        <section id="widget-grid" class="sayfaust_bosluk">
            <div class="row">
                <article class="col-sm-12 col-md-12 col-lg-12">
                    <div class="jarviswidget" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-togglebutton="false" data-widget-deletebutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-sortable="false">
                        <header>
                            <span class="widget-icon"> <i class="fa fa-list"></i> </span>
                            <h2>Hatırlatma Listesi</h2>

                        </header>
                        <div role="content" style="display: block;">
                            <div class="jarviswidget-editbox">
                            </div>
                            <div class="widget-body">
                                <div class="row">
                                    <div id="wizard-1" novalidate="novalidate">
                                        <div id="bootstrap-wizard-1" class="col-sm-12">
                                            @Html.AntiForgeryToken()
                                            <div id="wizard">
                                                <div class="form-bootstrapWizard">
                                                    <ul class="form-wizard nav nav-tabs" id="myTab">

                                                        <li data-target="#step1" class="active" id="step1" style="width:20%;">
                                                            <a href="#tab1" data-toggle="tab" aria-expanded="true">

                                                                <span class="title" style="z-index:2;">Ödemesi Yaklaşan Tahakkuklar</span>
                                                            </a>
                                                        </li>

                                                        <li data-target="#step2" id="step2" style="width:25%;">
                                                            <a href="#tab2" data-toggle="tab" aria-expanded="false">

                                                                <span class="title" style="z-index:2;">Ödemesi Geciken Tahakkuklar</span>
                                                            </a>
                                                        </li>

                                                        <li data-target="#step3" id="step3" style="width: 25%;">
                                                            <a href="#tab3" data-toggle="tab" aria-expanded="false">

                                                                <span class="title" style="z-index:2;">Sözleşme Süresi Dolan Beyanlar</span>
                                                            </a>
                                                        </li>

                                                        <li data-target="#step4" id="step4" style="width: 20%;">
                                                            <a href="#tab4" data-toggle="tab" aria-expanded="false">
                                                                <span class="title" style="z-index:2;">Artışı Gelen Beyanlar</span>
                                                            </a>
                                                        </li>

                                                    </ul>
                                                    <div class="clearfix"></div>
                                                </div>
                                                <div class="tab-content">
                                                    <div class="tab-pane active" id="tab1">
                                                        <legend>
                                                            <h3><strong>1. Adım </strong> - Ödemesi Yaklaşan Tahakkuklar</h3>
                                                        </legend>
                                                        <form class="form-horizontal" id="odemesiGelenForm" novalidate="novalidate">
                                                            @Html.AntiForgeryToken()
                                                            <fieldset>
                                                                <table id="dtGelen" class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" aria-describedby="dtTahakkuk_info">
                                                                    <thead>
                                                                        <tr role="row">
                                                                            <th style="width: 20px;">No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Gayrimenkul No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Sicil No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Ad Soyad/Tanım</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Beyan No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Beyan Yıl</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Ay</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Tahakkuk Tarihi</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Vade Tarihi</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Tutar</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">ödeme Bilgisi</th>
                                                                        </tr>
                                                                    </thead>

                                                                    @if (Model.OdemesiGelenler != null && Model.OdemesiGelenler.Count > 0)
                                                                    {
                                                                        <tbody>
                                                                            @{int i = ((Model.PageNumber - 1) * Model.PageSize) + 1;}

                                                                            @foreach (var item in Model.OdemesiGelenler)
                                                                            {
                                                                            <tr role="row" class="odd">
                                                                                <td>@(i++)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.GayrimenkulNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.SicilNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.Ad)</td>

                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Beyanlar.BeyanNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Beyanlar.BeyanYil)</td>
                                                                                <td>
                                                                                    @{
                                                                                        string monthName = item.VadeTarihi.Value.ToString("MMMM");
                                                                                        <span>@monthName</span>
                                                                                    }
                                                                                </td>
                                                                                <td>
                                                                                    @if (item.TahakkukTarihi.HasValue)
                                                                                    {
                                                                                        string basTarihi = item.TahakkukTarihi.Value.ToString("dd.MM.yyyy");
                                                                                        @Html.DisplayFor(model => basTarihi)
                                                                                    }
                                                                                </td>
                                                                                <td>
                                                                                    @if (item.VadeTarihi.HasValue)
                                                                                    {
                                                                                        string basTarihi = item.VadeTarihi.Value.ToString("dd.MM.yyyy");
                                                                                        @Html.DisplayFor(model => basTarihi)
                                                                                    }
                                                                                </td>
                                                                                <td>@Html.DisplayFor(model => item.Tutar)</td>
                                                                                <td>
                                                                                    @if (item.OdemeDurumu.HasValue && item.OdemeDurumu.Value)
                                                                                    {
                                                                                        <b class="label aktif-buton-beyandetay">Tahsil Edildi</b>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <b class="label pasif-buton-beyandetay">Vadesi Gelmeyen</b>
                                                                                    }
                                                                                </td>
                                                                            </tr>
                                                                            }
                                                                        </tbody>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tbody>
                                                                        </tbody>
                                                                    }
                                                                </table>
                                                            </fieldset>
                                                        </form>
                                                    </div>

                                                    <div class="tab-pane" id="tab2">
                                                        <legend>
                                                            <h3><strong>2. Adım </strong> - Ödemesi Geciken Tahakkuklar</h3>
                                                        </legend>
                                                        <form class="form-horizontal" id="gecikenForm" novalidate="novalidate">
                                                            @Html.AntiForgeryToken()
                                                            <fieldset>
                                                                <table id="dtGeciken" class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" aria-describedby="dtTahakkuk_info">
                                                                    <thead>
                                                                        <tr role="row">
                                                                            <th style="width: 20px;">No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Gayrimenkul No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Sicil No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Ad Soyad/Tanım</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Beyan No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Beyan Yıl</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Ay</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Tahakkuk Tarihi</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Vade Tarihi</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Tutar</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">ödeme Bilgisi</th>
                                                                        </tr>
                                                                    </thead>

                                                                    @if (Model.OdemesiGecikenler != null && Model.OdemesiGecikenler.Count > 0)
                                                                    {
                                                                        <tbody>
                                                                            @{int i = ((Model.PageNumber - 1) * Model.PageSize) + 1;}

                                                                            @foreach (var item in Model.OdemesiGecikenler)
                                                                            {
                                                                            <tr role="row" class="odd">
                                                                                <td>@(i++)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.GayrimenkulNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.SicilNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.Ad)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Beyanlar.BeyanNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Beyanlar.BeyanYil)</td>
                                                                                <td>
                                                                                    @{
                                                                                        string monthName = item.VadeTarihi.Value.ToString("MMMM");
                                                                                        <span>@monthName</span>
                                                                                    }
                                                                                </td>
                                                                                <td>
                                                                                    @if (item.TahakkukTarihi.HasValue)
                                                                                    {
                                                                                        string basTarihi = item.TahakkukTarihi.Value.ToString("dd.MM.yyyy");
                                                                                        @Html.DisplayFor(model => basTarihi)
                                                                                    }
                                                                                </td>
                                                                                <td>
                                                                                    @if (item.VadeTarihi.HasValue)
                                                                                    {
                                                                                        string basTarihi = item.VadeTarihi.Value.ToString("dd.MM.yyyy");
                                                                                        @Html.DisplayFor(model => basTarihi)
                                                                                    }
                                                                                </td>
                                                                                <td>@Html.DisplayFor(model => item.Tutar)</td>
                                                                                <td>
                                                                                    @if (item.OdemeDurumu.HasValue && item.OdemeDurumu.Value)
                                                                                    {
                                                                                        <b class="label aktif-buton-beyandetay">Tahsil Edildi</b>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <b class="label pasif-buton-beyandetay">Vadesi Gelmeyen</b>
                                                                                    }
                                                                                </td>
                                                                            </tr>
                                                                            }
                                                                        </tbody>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tbody>
                                                                        </tbody>
                                                                    }
                                                                </table>
                                                            </fieldset>
                                                        </form>
                                                    </div>

                                                    <div class="tab-pane" id="tab3">
                                                        <legend>
                                                            <h3><strong>3. Adım </strong> - Sözleşme Süresi Dolan Beyanlar</h3>
                                                        </legend>
                                                        <form class="form-horizontal" id="suresiDolanForm" novalidate="novalidate">
                                                            @Html.AntiForgeryToken()
                                                            <fieldset>
                                                                <table id="dtSuresiDolan" class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" aria-describedby="dtTahakkuk_info">
                                                                    <thead>
                                                                        <tr role="row">
                                                                            <th style="width: 20px;">No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Tür Adı</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Beyan No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Sicil No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Kiracı Bilgi</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Gayrimenkul No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">İl</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">İlçe</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Mahalle</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Durum</th>
                                                                            <th class="sorting_disabled text-center" rowspan="1" colspan="1" style="text-align: right;  padding-right: 30px; width: 100px;">İşlem</th>
                                                                        </tr>
                                                                    </thead>

                                                                    @if (Model.SozlesmesiBitenler != null && Model.SozlesmesiBitenler.Count > 0)
                                                                    {
                                                                        <tbody>
                                                                            @{int i = ((Model.PageNumber - 1) * Model.PageSize) + 1;}

                                                                            @foreach (var item in Model.SozlesmesiBitenler)
                                                                            {
                                                                            <tr role="row" class="odd">
                                                                                <td>@(i++)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.GayrimenkulTur.Ad)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Beyanlar.BeyanNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.SicilNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.Ad)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.GayrimenkulNo)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.Ilceler.Iller.Ad)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.Ilceler.Ad)</td>
                                                                                <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.Mahalleler.Ad)</td>
                                                                                <td>
                                                                                    @if (item.Kira_Beyani.Beyanlar.AktifMi == (int)Framework.Entities.Enums.EnmIslemDurumu.Aktif)
                                                                                    {
                                                                                        <b class="label aktif-buton-beyandetay">Aktif</b>
                                                                                    }
                                                                                    else if (item.Kira_Beyani.Beyanlar.AktifMi == (int)Framework.Entities.Enums.EnmIslemDurumu.Pasif)
                                                                                    {
                                                                                        <b class="label pasif-buton-beyandetay">Pasif</b>
                                                                                    }
                                                                                    else if (item.Kira_Beyani.Beyanlar.AktifMi == (int)Framework.Entities.Enums.EnmIslemDurumu.Kapandı)
                                                                                    {
                                                                                        <b class="label kapandi-buton-beyandetay">Kapandı</b>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <b class="label silindi-buton-beyandetay">Silindi</b>
                                                                                    }
                                                                                </td>
                                                                                <td class="text-center">

                                                                                    @Html.AuthAction(" ", "BeyanDetay", "Beyan", new { item.Kira_Beyani.Beyanlar.Guid },
                                                                                                    new { @class = "no-underline margin-right-5 fa fa-search-plus font-md txt-color-blue",
                                                                                                        @data_name = item.Kira_Beyani.Beyanlar.BeyanNo, @data_id = item.Kira_Beyani.Beyan_Id},
                                                                                                    Convert.ToBoolean(TempData["OpenAuthorizationPopup"]))

                                                                                </td>
                                                                            </tr>
                                                                            }
                                                                        </tbody>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tbody>
                                                                        </tbody>
                                                                    }
                                                                </table>
                                                            </fieldset>
                                                        </form>
                                                    </div>

                                                    <div class="tab-pane" id="tab4">
                                                        <legend>
                                                            <h3><strong>4. Adım </strong> -Artışı Gelen Beyanlar</h3>
                                                        </legend>
                                                        <form class="form-horizontal" id="artisiGelenForm" novalidate="novalidate">
                                                            <fieldset>
                                                                <table id="dtArtisiGelen" class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" aria-describedby="dtTahakkuk_info">
                                                                    <thead>
                                                                        <tr role="row">
                                                                            <th style="width: 20px;">No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Tür Adı</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Beyan No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Sicil No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Kiracı Bilgi</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Gayrimenkul No</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">İl</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">İlçe</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Mahalle</th>
                                                                            <th class="sorting_disabled" rowspan="1" colspan="1">Durum</th>
                                                                            <th class="sorting_disabled text-center" rowspan="1" colspan="1" style="text-align: right;  padding-right: 30px; width: 100px;">İşlem</th>
                                                                        </tr>
                                                                    </thead>

                                                                    @if (Model.ArtisiGelenler != null && Model.ArtisiGelenler.Count > 0)
                                                                    {
                                                                <tbody>
                                                                    @{int i = ((Model.PageNumber - 1) * Model.PageSize) + 1;}

                                                                    @foreach (var item in Model.SozlesmesiBitenler)
                                                                    {
                                                                        <tr role="row" class="odd">
                                                                            <td>@(i++)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.GayrimenkulTur.Ad)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Beyanlar.BeyanNo)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.SicilNo)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Kiracilar.Ad)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.GayrimenkulNo)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.Ad)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.Ilceler.Iller.Ad)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.Ilceler.Ad)</td>
                                                                            <td>@Html.DisplayFor(model => item.Kira_Beyani.Gayrimenkuller.Mahalleler.Ad)</td>
                                                                            <td>
                                                                                @if (item.Kira_Beyani.Beyanlar.AktifMi == (int)Framework.Entities.Enums.EnmIslemDurumu.Aktif)
                                                                                {
                                                                                    <b class="label aktif-buton-beyandetay">Aktif</b>
                                                                                }
                                                                                else if (item.Kira_Beyani.Beyanlar.AktifMi == (int)Framework.Entities.Enums.EnmIslemDurumu.Pasif)
                                                                                {
                                                                                    <b class="label pasif-buton-beyandetay">Pasif</b>
                                                                                }
                                                                                else if (item.Kira_Beyani.Beyanlar.AktifMi == (int)Framework.Entities.Enums.EnmIslemDurumu.Kapandı)
                                                                                {
                                                                                    <b class="label kapandi-buton-beyandetay">Kapandı</b>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <b class="label silindi-buton-beyandetay">Silindi</b>
                                                                                }
                                                                            </td>
                                                                            <td class="text-center">

                                                                                @Html.AuthAction(" ", "BeyanDetay", "Beyan", new { item.Kira_Beyani.Beyanlar.Guid },
                                                                                                new
                                                                                                     {
                                                                                                         @class = "no-underline margin-right-5 fa fa-search-plus font-md txt-color-blue",
                                                                                                    @data_name = item.Kira_Beyani.Beyanlar.BeyanNo,
                                                                                                         @data_id = item.Kira_Beyani.Beyan_Id
                                                                                                     },
                                                                                                Convert.ToBoolean(TempData["OpenAuthorizationPopup"]))

                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tbody>
                                                                        </tbody>
                                                                    }
                                                                </table>
                                                            </fieldset>
                                                        </form>
                                                    </div>
                                                </div>
                                                <div class="form-actions sonStep" style="margin-right: -18px;">
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <ul class="pager wizard no-margin no-steps-container">
                                                                <li class="previous disabled">
                                                                    <a href="javascript:void(0);" class="btn btn-lg btn-default" id="btnGeri"> Geri </a>
                                                                </li>
                                                                <li class="next">
                                                                    <a href="javascript:void(0);" id="btnIleri" class="btn btn-lg txt-color-darken"> İlerle</a>
                                                                    <div id="loadingbtnIleri" class="hide" style="float:right;"><i class="fa fa-spinner fa-spin fa-2x"></i> İşleminiz yapılıyor ...</div>
                                                                </li>
                                                                <li class="finish" style="float: right; margin-right: 15px; display: block;">
                                                                    <div id="dvSonKaydet" class="hide" style="float:right;"><i class="fa fa-spinner fa-spin fa-2x"></i> İşleminiz yapılıyor ...</div>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </article>
            </div>
        </section>
    </div>

}
@section pagespecific {

    <script src="~/Scripts/plugin/bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>
    <script src="~/Scripts/plugin/bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>

    <style type="text/css">

        .pasif-buton-beyandetay {
            padding: 3px 6px;
            border: solid 2px;
            background: none;
            border-color: #e08d26;
            color: #e08d26;
            font-weight: 700;
            display: block;
            text-align: center;
            font-size: 12px;
            width: 115px;
        }

        .silindi-buton-beyandetay {
            padding: 3px 3px;
            border: solid 2px;
            background: none;
            border-color: #ff0000;
            color: #ff0000;
            font-weight: 700;
            display: block;
            text-align: center;
            font-size: 12px;
            width: 75px;
        }

        .aktif-buton-beyandetay {
            padding: 3px 6px;
            border: solid 2px;
            background: none;
            border-color: #2ecc71;
            color: #2ecc71;
            font-weight: 700;
            display: block;
            text-align: center;
            font-size: 12px;
            width: 110px;
        }
    </style>

    <!-- Tablo Ayarlar-->
    <script type="text/javascript">

        var urlValue = '';

        var dt_settings = {
            "language": {
                "url": "//cdn.datatables.net/plug-ins/1.10.20/i18n/Turkish.json"
            },
            "searching": true
        };

        function getUrlValue() {

            const queryString = window.location.search;

            const urlParams = new URLSearchParams(queryString);

            var parameterValue = urlParams.get('tab');

            return parameterValue;
        }

        $(document).ready(function () {

            $('#dtGeciken').DataTable(dt_settings);

            $('#dtGelen').DataTable(dt_settings);

            $('#dtArtisiGelen').DataTable(dt_settings);

            $('#dtSuresiDolan').DataTable(dt_settings);

            urlValue = getUrlValue();

            if (urlValue) {
                $('a[href="#'+urlValue+'"]').click();
            }

        });
    </script>

    <!--Tab Control-->
    <script type="text/javascript">

        $(document).ready(function () {
            const wizard = $('#wizard');

            wizard.bootstrapWizard({
                'tabClass': 'nav nav-pills',
                onTabClick: function (tab, navigation, index) {
                    return true;
                },
                onNext: function (tab, navigation, index) {

                    if (index == 1) {
                        return true;
                    }
                    else if (index == 2) {
                        return true;
                    }
                    else if (index == 3) {
                        return true;
                    }
                    else if (index == 4) {
                        return false;
                    }

                },
                onTabShow: function (tab, navigation, index) {

                    const $total = navigation.find('li').length;
                    const $current = index + 1;
                    const pager = wizard.find('.pager');

                    if ($current >= $total) {
                        pager.find('.next').hide();
                        pager.find('.finish').show();
                        pager.find('.finish').removeClass('disabled');

                    } else {
                        pager.find('.next').show();
                        pager.find('.finish').hide();
                    }
                }
            });
        });
    </script>

}